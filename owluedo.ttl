@prefix : <https://w3id.org/cluedo4KG/onto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/cluedo4KG/onto#> .

<https://w3id.org/cluedo4KG/onto> rdf:type owl:Ontology ;
                                   <http://creativecommons.org/ns#license> <http://creativecommons.org/licenses/by/4.0/> ;
                                   <http://purl.org/dc/terms/creator> "Camille Pradel" ,
                                                                      "Nathalie Hernandez" ;
                                   <http://purl.org/dc/terms/issued> "2012-12-01"^^xsd:date ;
                                   <http://purl.org/vocab/vann/preferredNamespacePrefix> "cluedonto" ;
                                   <http://purl.org/vocab/vann/preferredNamespaceUri> : ;
                                   rdfs:comment "Cette ontologie a été créée pour être utilisée dans le tutoriel SPARQLUEDO faisant partie de la ressource CLUEDO4KG"@fr ,
                                                "This ontology was created for use in the SPARQLUEDO tutorial, which is part of the CLUEDO4KG resource."@en .

#################################################################
#    Annotation properties
#################################################################

###  http://creativecommons.org/ns#license
<http://creativecommons.org/ns#license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
<http://purl.org/dc/terms/issued> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/cluedo4KG/onto#aPourPieceVoisine
:aPourPieceVoisine rdf:type owl:ObjectProperty ,
                            owl:SymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain :Piece ;
                   rdfs:range :Piece ;
                   rdfs:label "a pour pièce voisine"@fr ,
                              "has as neighbour room"@en .


###  https://w3id.org/cluedo4KG/onto#estCentreDe
:estCentreDe rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :zone_autour_immobile .


###  https://w3id.org/cluedo4KG/onto#etage_contient_pièce
:etage_contient_pièce rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :lieu_contient_lieu ;
                      owl:inverseOf :piece_dans_etage .


###  https://w3id.org/cluedo4KG/onto#etage_dans_maison
:etage_dans_maison rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :lieu_dans_lieu ;
                   owl:inverseOf :maison_contient_etage ;
                   rdfs:domain :etage ;
                   rdfs:range :Maison .


###  https://w3id.org/cluedo4KG/onto#lieu_contient_lieu
:lieu_contient_lieu rdf:type owl:ObjectProperty ;
                    owl:inverseOf :lieu_dans_lieu ;
                    rdf:type owl:TransitiveProperty ;
                    rdfs:domain :lieux ;
                    rdfs:range :lieux .


###  https://w3id.org/cluedo4KG/onto#lieu_dans_lieu
:lieu_dans_lieu rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain :lieux ;
                rdfs:range :lieux .


###  https://w3id.org/cluedo4KG/onto#maisonContientPiece
:maisonContientPiece rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :lieu_contient_lieu ;
                     owl:inverseOf :pieceDansMaison ;
                     rdfs:domain :Maison ;
                     rdfs:range :Piece ;
                     rdfs:label "house contains room"@en ,
                                "maison contient pièce"@fr .


###  https://w3id.org/cluedo4KG/onto#maison_contient_etage
:maison_contient_etage rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :lieu_contient_lieu .


###  https://w3id.org/cluedo4KG/onto#meurtrier_a_tué_victime
:meurtrier_a_tué_victime rdf:type owl:ObjectProperty ;
                         owl:inverseOf :victime_tuée_par_meutrier .


###  https://w3id.org/cluedo4KG/onto#mort_associé_objet
:mort_associé_objet rdf:type owl:ObjectProperty ;
                    owl:inverseOf :objet_associé_mort ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain :causeMort ;
                    rdfs:range :objetMobile .


###  https://w3id.org/cluedo4KG/onto#mort_par_balle
:mort_par_balle rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :mort_associé_objet ;
                rdfs:domain :blessureParBalle ;
                rdfs:range :armesAFeu .


###  https://w3id.org/cluedo4KG/onto#objetDansPiece
:objetDansPiece rdf:type owl:ObjectProperty ;
                owl:inverseOf :pieceContientObjet ;
                rdfs:label "object in room"@en ,
                           "objet dans pièce"@fr .


###  https://w3id.org/cluedo4KG/onto#objet_actuellement_possession
:objet_actuellement_possession rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :objet_possession_personne ;
                               rdfs:domain :Objet ;
                               rdfs:range :Personne .


###  https://w3id.org/cluedo4KG/onto#objet_associé_mort
:objet_associé_mort rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty .


###  https://w3id.org/cluedo4KG/onto#objet_possession_personne
:objet_possession_personne rdf:type owl:ObjectProperty ;
                           owl:inverseOf :personne_possède_objet ;
                           rdfs:domain :Objet ;
                           rdfs:range :Personne .


###  https://w3id.org/cluedo4KG/onto#personneDansPiece
:personneDansPiece rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :personne_dans_lieu ;
                   owl:inverseOf :pieceContientPersonne ;
                   owl:propertyChainAxiom ( :personne_possède_objet
                                            :objetDansPiece
                                          ) ;
                   rdfs:label "person in room"@en ,
                              "personne dans pièce"@fr .


###  https://w3id.org/cluedo4KG/onto#personne_actuellement_dans_lieu
:personne_actuellement_dans_lieu rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf :personne_dans_lieu .


###  https://w3id.org/cluedo4KG/onto#personne_actuellement_dans_pièce
:personne_actuellement_dans_pièce rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf :personneDansPiece ,
                                                     :personne_actuellement_dans_lieu ;
                                  rdfs:domain :Personne ;
                                  rdfs:range :Piece .


###  https://w3id.org/cluedo4KG/onto#personne_dans_lieu
:personne_dans_lieu rdf:type owl:ObjectProperty ;
                    rdfs:domain :Personne ;
                    rdfs:range :lieux ;
                    owl:propertyChainAxiom ( :personne_dans_lieu
                                             :lieu_dans_lieu
                                           ) .


###  https://w3id.org/cluedo4KG/onto#personne_possede_actuellement_objet
:personne_possede_actuellement_objet rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf :personne_possède_objet .


###  https://w3id.org/cluedo4KG/onto#personne_possède_objet
:personne_possède_objet rdf:type owl:ObjectProperty .


###  https://w3id.org/cluedo4KG/onto#pieceContientObjet
:pieceContientObjet rdf:type owl:ObjectProperty ;
                    rdfs:domain :Piece ;
                    rdfs:range :Objet ;
                    owl:propertyChainAxiom ( :piece_contient_zone
                                             :zone_autour_immobile
                                           ) ;
                    rdfs:label "pièce contient objet"@fr ,
                               "room contains object"@en .


###  https://w3id.org/cluedo4KG/onto#pieceContientPersonne
:pieceContientPersonne rdf:type owl:ObjectProperty ;
                       rdfs:domain :Piece ;
                       rdfs:range :Personne ;
                       rdfs:label "pièce contient personne"@fr ,
                                  "room contains person"@en .


###  https://w3id.org/cluedo4KG/onto#pieceDansMaison
:pieceDansMaison rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :lieu_dans_lieu ;
                 rdfs:label "pièce dans maison"@fr ,
                            "room in house"@en .


###  https://w3id.org/cluedo4KG/onto#piece_contient_zone
:piece_contient_zone rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :lieu_contient_lieu ;
                     rdfs:domain :Piece ;
                     rdfs:range :zones .


###  https://w3id.org/cluedo4KG/onto#piece_dans_etage
:piece_dans_etage rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :lieu_dans_lieu ;
                  rdfs:domain :Piece ;
                  rdfs:range :etage .


###  https://w3id.org/cluedo4KG/onto#victime_cause_deces
:victime_cause_deces rdf:type owl:ObjectProperty ;
                     rdfs:domain :victime ;
                     rdfs:range :causeMort .


###  https://w3id.org/cluedo4KG/onto#victime_tuée_par_meutrier
:victime_tuée_par_meutrier rdf:type owl:ObjectProperty ;
                           rdfs:domain :victime ;
                           rdfs:range :meurtrier .


###  https://w3id.org/cluedo4KG/onto#zone_autour_immobile
:zone_autour_immobile rdf:type owl:ObjectProperty ;
                      rdfs:domain :zones ;
                      rdfs:range :objetImmobile .


###  https://w3id.org/cluedo4KG/onto#zone_dans_piece
:zone_dans_piece rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :lieu_dans_lieu ;
                 rdfs:domain :zones ;
                 rdfs:range :Piece ;
                 owl:propertyChainAxiom ( :objetDansPiece
                                          :zone_autour_immobile
                                        ) .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/cluedo4KG/onto#densité_produit
:densité_produit rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :produit ;
                 rdfs:range xsd:float .


###  https://w3id.org/cluedo4KG/onto#estVivant
:estVivant rdf:type owl:DatatypeProperty ;
           rdfs:domain :Personne ;
           rdfs:range xsd:boolean ;
           rdfs:comment "indicates whether a person is alive (True) or dead (False)"@en ,
                        "indique si une personne est vivante (True) ou morte (False)"@fr ;
           rdfs:label "est vivant"@fr ,
                      "is alive"@en .


###  https://w3id.org/cluedo4KG/onto#est_mort_depuis
:est_mort_depuis rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Personne ;
                 rdfs:range xsd:integer .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/cluedo4KG/onto#Maison
:Maison rdf:type owl:Class ;
        rdfs:subClassOf :lieux ;
        owl:disjointWith :etage ;
        rdfs:comment "house"@en ;
        rdfs:label "maison"@fr .


###  https://w3id.org/cluedo4KG/onto#Objet
:Objet rdf:type owl:Class ;
       rdfs:subClassOf owl:Thing ;
       rdfs:label "object"@en ,
                  "objet"@fr .


###  https://w3id.org/cluedo4KG/onto#Personne
:Personne rdf:type owl:Class ;
          rdfs:subClassOf owl:Thing ;
          owl:disjointUnionOf ( :innocent
                                :meurtrier
                                :victime
                              ) ;
          rdfs:label "person"@en ,
                     "personne"@fr .


###  https://w3id.org/cluedo4KG/onto#Piece
:Piece rdf:type owl:Class ;
       rdfs:subClassOf :lieux ;
       owl:disjointWith :etage ;
       rdfs:label "pièce"@fr ,
                  "room"@en .


###  https://w3id.org/cluedo4KG/onto#armeDuCrime
:armeDuCrime rdf:type owl:Class ;
             rdfs:subClassOf :armes .


###  https://w3id.org/cluedo4KG/onto#armes
:armes rdf:type owl:Class ;
       rdfs:subClassOf :objetMobile .


###  https://w3id.org/cluedo4KG/onto#armesAFeu
:armesAFeu rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( :Revolver_Python_357
                                           )
                               ] ;
           rdfs:subClassOf :armes .


###  https://w3id.org/cluedo4KG/onto#autreVictime
:autreVictime rdf:type owl:Class ;
              rdfs:subClassOf :victime .


###  https://w3id.org/cluedo4KG/onto#blessureParBalle
:blessureParBalle rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty :mort_associé_objet ;
                                        owl:someValuesFrom :armesAFeu
                                      ] ;
                  rdfs:subClassOf :blessures .


###  https://w3id.org/cluedo4KG/onto#blessureParObjetContondant
:blessureParObjetContondant rdf:type owl:Class ;
                            rdfs:subClassOf :blessures .


###  https://w3id.org/cluedo4KG/onto#blessureParObjetTranchant
:blessureParObjetTranchant rdf:type owl:Class ;
                           rdfs:subClassOf :blessures .


###  https://w3id.org/cluedo4KG/onto#blessures
:blessures rdf:type owl:Class ;
           rdfs:subClassOf :causeMort .


###  https://w3id.org/cluedo4KG/onto#causeMort
:causeMort rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( :uniqueCauseMort
                                           )
                               ] .


###  https://w3id.org/cluedo4KG/onto#contondant
:contondant rdf:type owl:Class ;
            rdfs:subClassOf :armes .


###  https://w3id.org/cluedo4KG/onto#empoisonnement
:empoisonnement rdf:type owl:Class ;
                rdfs:subClassOf :causeMort .


###  https://w3id.org/cluedo4KG/onto#etage
:etage rdf:type owl:Class ;
       rdfs:subClassOf :lieux .


###  https://w3id.org/cluedo4KG/onto#innocent
:innocent rdf:type owl:Class ;
          rdfs:subClassOf :Personne .


###  https://w3id.org/cluedo4KG/onto#lieuDuCrime
:lieuDuCrime rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Piece
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :pieceContientPersonne ;
                                                          owl:someValuesFrom :victime
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :lieux .


###  https://w3id.org/cluedo4KG/onto#lieux
:lieux rdf:type owl:Class .


###  https://w3id.org/cluedo4KG/onto#liquide
:liquide rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :densité_produit ;
                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                    owl:onDatatype xsd:float ;
                                                    owl:withRestrictions ( [ xsd:maxExclusive "2.5"^^xsd:float
                                                                           ]
                                                                         )
                                                  ]
                             ] ;
         rdfs:subClassOf :produit .


###  https://w3id.org/cluedo4KG/onto#meurtrier
:meurtrier rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Personne
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :meurtrier_a_tué_victime ;
                                                        owl:someValuesFrom :victime
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ,
                               [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty :personneDansPiece ;
                                                        owl:someValuesFrom :lieuDuCrime
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :personne_possède_objet ;
                                                        owl:someValuesFrom :armeDuCrime
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :Personne ;
           rdfs:label "assassin"@fr ,
                      "meutrier"@fr ,
                      "murderer"@en ,
                      "tueur"@fr .


###  https://w3id.org/cluedo4KG/onto#mou
:mou rdf:type owl:Class ;
     rdfs:subClassOf :objetMobile .


###  https://w3id.org/cluedo4KG/onto#noyade
:noyade rdf:type owl:Class ;
        rdfs:subClassOf :causeMort .


###  https://w3id.org/cluedo4KG/onto#objetImmobile
:objetImmobile rdf:type owl:Class ;
               rdfs:subClassOf :Objet .


###  https://w3id.org/cluedo4KG/onto#objetMobile
:objetMobile rdf:type owl:Class ;
             rdfs:subClassOf :Objet .


###  https://w3id.org/cluedo4KG/onto#poudre
:poudre rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :densité_produit ;
                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                   owl:onDatatype xsd:float ;
                                                   owl:withRestrictions ( [ xsd:minExclusive "2.5"^^xsd:float
                                                                          ]
                                                                        )
                                                 ]
                            ] ;
        rdfs:subClassOf :produit .


###  https://w3id.org/cluedo4KG/onto#produit
:produit rdf:type owl:Class ;
         rdfs:subClassOf :objetMobile ;
         owl:disjointUnionOf ( :liquide
                               :poudre
                             ) .


###  https://w3id.org/cluedo4KG/onto#tranchant
:tranchant rdf:type owl:Class ;
           rdfs:subClassOf :armes .


###  https://w3id.org/cluedo4KG/onto#victime
:victime rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :Personne
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :victime_cause_deces ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :causeMort
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ,
                             [ owl:intersectionOf ( :Personne
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :estVivant ;
                                                      owl:hasValue "false"^^xsd:boolean
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf :Personne ;
         owl:disjointUnionOf ( :autreVictime
                               :victimeParBalle
                             ) .


###  https://w3id.org/cluedo4KG/onto#victimeParBalle
:victimeParBalle rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :victime
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :victime_cause_deces ;
                                                              owl:someValuesFrom :blessureParBalle
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ,
                                     [ owl:intersectionOf ( :victime
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :est_mort_depuis ;
                                                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                   owl:onDatatype xsd:integer ;
                                                                                   owl:withRestrictions ( [ xsd:maxExclusive 5
                                                                                                          ]
                                                                                                        )
                                                                                 ]
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :victime .


###  https://w3id.org/cluedo4KG/onto#zones
:zones rdf:type owl:Class ;
       rdfs:subClassOf :lieux .


#################################################################
#    Individuals
#################################################################

###  http://www.lamaisondumeurtre.fr#2eme_Etage
<http://www.lamaisondumeurtre.fr#2eme_Etage> rdf:type owl:NamedIndividual ,
